{
  "name": "strider-github",
  "version": "0.1.1",
  "description": "A basic git provider for strider",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-github.git"
  },
  "keywords": [
    "git",
    "strider"
  ],
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "Readme.md",
  "strider": {
    "id": "github",
    "title": "Github",
    "type": "provider",
    "hosted": true,
    "config": {
      "controller": "GithubCtrl"
    },
    "accountConfig": {
      "setupLink": "/auth/github"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "github"
  },
  "devDependencies": {
    "jshint": "~2.1.11",
    "expect.js": "~0.2.0",
    "mocha": "~1.13.0"
  },
  "dependencies": {
    "passport": "~0.1.17",
    "passport-github": "~0.1.5",
    "superagent": "~0.15.4",
    "strider-git": "~0.1.0",
    "request": "~2.27.0",
    "lodash": "~2.2.0",
    "step": "0.0.5",
    "async": "~0.2.9",
    "crypto": "0.0.3",
    "gravatar": "~1.0.6",
    "debug": "~0.7.3"
  }
}
