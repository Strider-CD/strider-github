{
  "name": "strider-github",
  "version": "2.3.4",
  "description": "A GibHub & GitHub Enterprise provider for Strider",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run tests",
    "lint": "eslint lib",
    "tests": "mocha --timeout 5000 -R spec test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-github.git"
  },
  "keywords": [
    "git",
    "strider"
  ],
  "engines": {
    "node": ">=4.2"
  },
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "strider": {
    "id": "github",
    "title": "Github",
    "type": "provider",
    "hosted": true,
    "config": {
      "controller": "GithubCtrl"
    },
    "accountConfig": {
      "setupLink": "/auth/github"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "github"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "expect.js": "~0.3.1",
    "mocha": "~3.5.0",
    "nock": "^9.0.14"
  },
  "dependencies": {
    "async": "~2.5.0",
    "debug": "~3.0.1",
    "gravatar": "~1.6.0",
    "lodash": "^4.17.4",
    "passport-github": "^1.1.0",
    "scmp": "2.0.0",
    "ssh-keypair": "~2.0.0",
    "step": "1.0.0",
    "strider-git": "^1.0.0",
    "superagent": "~3.6.0"
  }
}
