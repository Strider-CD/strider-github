{
  "name": "strider-github",
  "version": "3.0.3",
  "description": "A GibHub & GitHub Enterprise provider for Strider",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run tests",
    "lint": "eslint lib",
    "tests": "mocha --timeout 5000 -R spec test/",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-github.git"
  },
  "keywords": [
    "git",
    "strider"
  ],
  "engines": {
    "node": ">= 10.13.0 || >= 11.10.1"
  },
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "strider": {
    "id": "github",
    "title": "Github",
    "type": "provider",
    "hosted": true,
    "config": {
      "controller": "GithubCtrl"
    },
    "accountConfig": {
      "setupLink": "/auth/github"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "github"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "expect.js": "~0.3.1",
    "mocha": "^7.0.1",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  },
  "dependencies": {
    "async": "^3.1.1",
    "debug": "^4.1.1",
    "gravatar": "^1.8.0",
    "lodash": "^4.17.5",
    "passport-github": "^1.1.0",
    "scmp": "0.0.2",
    "ssh-keypair": "^2.0.0",
    "step": "^1.0.0",
    "strider-git": "^2.0.0",
    "superagent": "^5.2.1"
  }
}
