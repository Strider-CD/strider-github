{
  "name": "strider-github",
  "version": "2.3.3",
  "description": "A GibHub & GitHub Enterprise provider for Strider",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run tests",
    "lint": "eslint lib",
    "tests": "mocha --timeout 5000 -R spec test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-github.git"
  },
  "keywords": [
    "git",
    "strider"
  ],
  "engines": {
    "node": ">=4.2"
  },
  "author": "Jared Forsyth <jared@jaredforsyth.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "strider": {
    "id": "github",
    "title": "Github",
    "type": "provider",
    "hosted": true,
    "config": {
      "controller": "GithubCtrl"
    },
    "accountConfig": {
      "setupLink": "/auth/github"
    },
    "webapp": "lib/webapp.js",
    "worker": "lib/worker.js",
    "inline_icon": "github"
  },
  "devDependencies": {
    "eslint": "^3.1.1",
    "expect.js": "~0.2.0",
    "mocha": "~1.13.0",
    "nock": "^2.10.0"
  },
  "dependencies": {
    "async": "~0.2.9",
    "debug": "~2.2.0",
    "gravatar": "~1.0.6",
    "lodash": "^2.2.1",
    "passport-github": "^1.0.0",
    "scmp": "0.0.2",
    "ssh-keypair": "~1.0.0",
    "step": "0.0.5",
    "strider-git": "~0.2.4",
    "superagent": "~0.15.4"
  }
}
